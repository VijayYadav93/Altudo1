<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
	  <contentSearch>
		  <indexConfiguration>
			  <defaultSolrIndexConfiguration>
				  <fieldMap>
					  <fieldsNames hint="raw:AddFieldByFieldName">
						  <field fieldName ="HealthArticle" returnType="string"/>						  
					  </fieldsNames>
				  </fieldMap>
				  <documentOptions>
					  <fields hint="raw:AddComputedIndexField">
						  <field fileName="HealthArticle" returnType="string">
							  Altudobtc1.Foundation.Search.ComputedIndexes.ArticleUrl, Altudobtc1.Foundation.Search
						  </field>
					  </fields>
				  </documentOptions>
			  </defaultSolrIndexConfiguration>
		  </indexConfiguration>
	  </contentSearch>
  </sitecore>
</configuration>
