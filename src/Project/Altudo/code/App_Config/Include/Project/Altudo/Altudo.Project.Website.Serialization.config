<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
	<sitecore>
		<unicorn>
			<configurations>
				<configuration name="Project.Altudo" dependencies="Foundation.Serialization" description="All project level config like pages templates, layouts, basepage etc..., serialization here">
					<targetDataStore physicalRootPath="C:\Project\Altudo-1\src\$(layer)\$(module)\serialization" />
					<predicate>
						<include name="Templates" database="master" path="/sitecore/templates/Project/VisitUs" />
						<include name="Workflow" database="master" path="/sitecore/system/Workflows/VisitUs Workflow" />
						<include name="Languages" database="master" path="/sitecore/system/Languages" />
						<include name="ChunkChild" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Chunks/GetChildItem Count" />
						<include name="ChunkCurrent" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Chunks/Get Item Name" />
						<include name="StripHouseKeeping" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Strips/HouseKeeping" />
						<include name="RibbonHouseKeeping" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Ribbons/Default/HouseKeeping" />
					</predicate>
					<dataProviderConfiguration enableTransparentSync="false" />
					<syncConfiguration updateLinkDatabase="false" updateSearchIndex="false" />
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>
